<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.BackOffice.ListSponsorController">
   <children>
      <ListView fx:id="ListSponsors" layoutX="31.0" layoutY="139.0" onMouseClicked="#OnListClicked" prefHeight="537.0" prefWidth="505.0" />
      <Label layoutX="73.0" layoutY="40.0" prefHeight="85.0" prefWidth="464.0" text="Liste des sponsors">
         <font>
            <Font name="System Bold" size="46.0" />
         </font>
      </Label>
      <Button fx:id="BtnSupprimer" layoutX="390.0" layoutY="683.0" mnemonicParsing="false" onAction="#OnSupprimerClicked" prefHeight="31.0" prefWidth="146.0" styleClass="delete-button" text="Supprimer" />
      <Label layoutX="620.0" layoutY="213.0" text="Nom" />
      <Label layoutX="620.0" layoutY="283.0" text="Donnation" />
      <Label layoutX="634.0" layoutY="464.0" text="Image" />
      <TextField fx:id="tfNom" layoutX="729.0" layoutY="208.0" />
      <TextField fx:id="tfDonnation" layoutX="730.0" layoutY="278.0" />
      <ImageView fx:id="imageView" fitHeight="219.0" fitWidth="264.0" layoutX="692.0" layoutY="389.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="BtnInsererImage" layoutX="767.0" layoutY="330.0" mnemonicParsing="false" onAction="#OnInsererClicked" text="Inserer Image" />
      <Button fx:id="BtnGoPromo" layoutX="31.0" layoutY="683.0" mnemonicParsing="false" onAction="#GoProm" text="Voir Promotion" visible="false" />
      <HBox fx:id="HboxModif" alignment="CENTER" layoutX="620.0" layoutY="599.0" prefHeight="100.0" prefWidth="313.0" spacing="50.0" visible="false">
         <children>
            <Button fx:id="BtnModifier" mnemonicParsing="false" onAction="#OnModifierClicked" prefHeight="31.0" prefWidth="111.0" styleClass="update-button" text="Modifier" />
            <Button fx:id="BtnAnnuler" mnemonicParsing="false" onAction="#OnAnnulerClicked" prefHeight="31.0" prefWidth="112.0" text="Annuler" />
         </children>
      </HBox>
      <Button fx:id="BtnAjouter" layoutX="691.0" layoutY="634.0" mnemonicParsing="false" onAction="#OnAjouterClicked" prefHeight="31.0" prefWidth="153.0" styleClass="add-button" text="Ajouter" />
   </children>
</AnchorPane>
